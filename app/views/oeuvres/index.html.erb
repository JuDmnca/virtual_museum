<p id="notice"><%= notice %></p>

<h1>Oeuvres</h1>

<table>
  <thead>
    <tr>
      <th>Url</th>
      <th>Color</th>
      <th>Title</th>
      <th>Artist</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @oeuvres.each do |oeuvre| %>
      <tr>
        <td><%= oeuvre.url %></td>
        <td><%= oeuvre.color %></td>
        <td><%= oeuvre.title %></td>
        <td><%= oeuvre.artist %></td>
        <td><%= oeuvre.date %></td>
        <td><%= link_to 'Show', oeuvre %></td>
        <td><%= link_to 'Edit', edit_oeuvre_path(oeuvre) %></td>
        <td><%= link_to 'Destroy', oeuvre, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Oeuvre', new_oeuvre_path %>
